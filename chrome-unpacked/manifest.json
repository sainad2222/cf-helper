{
    "manifest_version": 2,
  
    "name": "cf-helper",
    "description": "Enables extra features for codeforces",
    "version": "2.0",
    "permissions": [
     "activeTab",
     "tabs",
     "background",
     "*://*.codeforces.com/",
     "*://*.atcoder.jp/"
     ],
     "icons":{
         "32":"static/logo32.png",
         "64":"static/logo64.png",
         "128":"static/logo128.png"
     },
     "content_scripts":
    [
        {
            "js": ["main.js"],
            "matches": ["*://*.codeforces.com/*","*://*.atcoder.jp/contests/*"]
        }
    ]
     
  }